<table class="table table-striped">
  <thead>
    <tr>
      <th>ID</th>
   	  <th>Order</th>
   	  <th>Name</th>
   	  <th>Phone</th>
   	  <th>Adress</th>
   	</tr>
  </thead>
  <tbody>
  <%@orders.each do |order|%>
   <tr>
   	  <td><b><%=order.id %></b></td>
   	  <td><%=order.orders_input %></td>
   	  <td><%=order.name %></td>
   	  <td><%=order.phone %></td>
   	  <td><%=order.address %></td>
   </tr>
   <% end %>
</table>