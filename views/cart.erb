<h2><center>Your cart</center></h2>
<table class="table table-condensed">
  <thead>
    <tr>
   	  <th>#</th>
   	  <th>Product</th>
   	  <th>Price</th>
   	  <th>Size</th>
   	  <th>Qty</th>
   	</tr>
  </thead>
  <tbody>
  <%@items.each do |item|%>
   <tr>
   	  <td><%= item[0].id %></td>
   	  <td><%= item[0].title %></td>
   	  <td><%= item[0].price %> $</td>
   	  <td><%= item[0].size %> cm</td>
   	  <td><%= item[1] %></td>
   </tr>
  <% end %>
  </tbody>
</table>



